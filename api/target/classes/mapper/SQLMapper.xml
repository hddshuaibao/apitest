<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.weixt.model">
    <select id="loginCase" parameterType="Integer" resultType="com.weixt.model.LoginCase" >
        select * from loginCase
        where id=#{id}
    </select>

    <select id="versionListCase" parameterType="Integer" resultType="com.weixt.model.GetParam">
        select * from param
        where id=#{id}
    </select>

    <select id="cost_type_configSaveCase" parameterType="Integer" resultType="com.weixt.model.GetParam">
        select * from  param where id=#{id}

    </select>
</mapper>